<Window x:Class="TreeCadN.filtero"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TreeCadN"
        mc:Ignorable="d"
        Title="filtero" Height="450" Width="800">
    <Grid>
        <DataGrid x:Name="fsds" HorizontalAlignment="Left" Height="375" Margin="93,10,0,0" VerticalAlignment="Top" Width="450">
            <DataGrid.Columns>
                <DataGridTextColumn Header="№ заказа" Binding="{Binding name}"  MinWidth="20" />
           </DataGrid.Columns>

        </DataGrid>

    </Grid>
</Window>
