<Window x:Class="TreeCadN.kommunikacii.kommunikacii_dial_imp"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TreeCadN.kommunikacii"
        mc:Ignorable="d"
        Title="Добавление пользовательского 3d объекта" Height="348.884" Width="506.172" WindowStartupLocation="CenterScreen" ResizeMode="CanMinimize">
    <Grid>
        <Button Content="Обзор" HorizontalAlignment="Left" Margin="417,77,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>
        <TextBox x:Name="tb1" HorizontalAlignment="Left" Height="23" Margin="11,76,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="401" ToolTip="Укажите путь к 3ds файлу"/>
        <TextBox x:Name="tb2" HorizontalAlignment="Left" Height="23" Margin="11,128,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="401" ToolTip="Укажите путь к Картинке, которая буте отображаться, при поиске объекта"/>
        <Button Content="Обзор" HorizontalAlignment="Left" Margin="417,128,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click_1"/>
        <Button Content="Сохранить" HorizontalAlignment="Left" Margin="415,278,0,0" VerticalAlignment="Top" Width="75" Height="31" Click="Button_Click_2"/>
        <TextBlock HorizontalAlignment="Left" Margin="11,59,0,0" TextWrapping="Wrap" Text="Выберите 3ds объект*" VerticalAlignment="Top"/>
        <TextBlock HorizontalAlignment="Left" Margin="11,110,0,0" TextWrapping="Wrap" Text="Добавьте картинку" VerticalAlignment="Top"/>
        <TextBox x:Name="tx" HorizontalAlignment="Left" Height="23" Margin="60,187,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="59" PreviewTextInput="tx_PreviewTextInput" PreviewKeyDown="tx_PreviewKeyDown"/>
        <TextBox x:Name="ty" HorizontalAlignment="Left" Height="23" Margin="191,187,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="59" PreviewKeyDown="ty_PreviewKeyDown" PreviewTextInput="ty_PreviewTextInput"/>
        <TextBox x:Name="tz" HorizontalAlignment="Left" Height="23" Margin="340,187,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="59" PreviewKeyDown="tz_PreviewKeyDown" PreviewTextInput="tz_PreviewTextInput"/>
        <TextBox x:Name="tb3" HorizontalAlignment="Left" Height="23" Margin="10,251,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="401" ToolTip="Картинка для отображения УГО на чертеже"/>
        <Button x:Name="btn1" Content="Обзор" HorizontalAlignment="Left" Margin="415,251,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click_3"/>
        <TextBlock HorizontalAlignment="Left" Margin="10,187,0,0" TextWrapping="Wrap" Text="Ширина" VerticalAlignment="Top"/>
        <TextBlock HorizontalAlignment="Left" Margin="147,187,0,0" TextWrapping="Wrap" Text="Высота" VerticalAlignment="Top"/>
        <TextBlock HorizontalAlignment="Left" Margin="290,187,0,0" TextWrapping="Wrap" Text="Глубина" VerticalAlignment="Top"/>
        <TextBlock HorizontalAlignment="Left" Margin="10,164,0,0" TextWrapping="Wrap" Text="Габариты" VerticalAlignment="Top" Width="100"/>
        <TextBlock HorizontalAlignment="Left" Margin="11,233,0,0" TextWrapping="Wrap" Text="Добавьте картинку для Условно Графического Обозначения (УГО)" VerticalAlignment="Top"/>
        <TextBox x:Name="tb4" HorizontalAlignment="Left" Height="23" Margin="10,25,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="480" ToolTip="Введите наименование объекта"/>
        <TextBlock HorizontalAlignment="Left" Margin="10,8,0,0" TextWrapping="Wrap" Text="Наименование" VerticalAlignment="Top"/>

    </Grid>
</Window>
